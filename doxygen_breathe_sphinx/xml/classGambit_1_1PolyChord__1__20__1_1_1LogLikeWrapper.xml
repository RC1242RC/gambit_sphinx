        <type>void</type>
        <definition>void Gambit::PolyChord_1_20_1::LogLikeWrapper::dumper</definition>
        <argsstring>(int, int, int, double *, double *, double *, double, double)</argsstring>
        <name>dumper</name>
        <param>
          <type>int</type>
          <defname>ndead</defname>
        </param>
        <param>
          <type>int</type>
          <defname>nlive</defname>
        </param>
        <param>
          <type>int</type>
          <defname>npars</defname>
        </param>
        <param>
          <type>double *</type>
          <defname>live</defname>
