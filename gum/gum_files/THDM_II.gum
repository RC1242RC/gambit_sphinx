## Minimal .gum (YAML) file for the THDM-II model in GUM

math:
  # Name of Mathematica package, either 'feynrules' or 'sarah'
  package: sarah
  # Name of Model within that package. This will also be the name of the GAMBIT model.
  model: THDM-II
  lagrangian: LTotal

# Outputs for GUM to hook up to GAMBIT
output:
  spheno: true
  vevacious: true

# Options for outputs
output_options:
  spheno:
    IncludeLoopDecays: false
